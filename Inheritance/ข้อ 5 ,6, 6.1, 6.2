import java.util.*;

public class Main
{
	public static void main(String[] args) {
	    
	    //6.1
		Account client1 = new Account(1123,20000,3.75);
		Scanner kb = new Scanner(System.in);
		
	    String name = kb.nextLine();
		String surname = kb.nextLine();
		client1.setName(name,surname);
		
		
		int d = kb.nextInt();
		kb.nextLine();
		String m = kb.nextLine();
		int y = kb.nextInt();
		client1.setDate(new Date(d,m,y));
		
		Date now = new Date(7,"Aug",2025);
		
	
	    client1.withdraw(2500);
	    client1.deposit(3000);
	    
	    Account client2 = new Account(1100,0,3);
	    
	    client1.transferMoney(client2,5000);
	   
	    System.out.println(client1.getBalance());
	    System.out.println(client1.getMonthlyInterest(now));

	    System.out.println("-------------------");
	    
	    
