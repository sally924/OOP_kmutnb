import java.util.*;

public class Main
{
	public static void main(String[] args) {
	    
	    Scanner kb = new Scanner(System.in);
	    int num = kb.nextInt();
	    
		for(int i = 1; i<=num; i++){
		    for(int j = 1;j<=num;j++){
		        
		       if(j*i != 1){
		          System.out.printf("%4d",j*i);
		       }else{
		          System.out.printf("    ");
		       }
		       
		       

		    }
		    System.out.println();
		}
	}
}
